{
  "name": "kinzal.net",
  "version": "1.0.0",
  "description": "Illustration portfolio of kinzal.",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite-react-ssg build && npm run postbuild",
    "postbuild": "node -e \"const fs=require('fs'); ['original','scrap'].forEach(f => { const src='dist/'+f+'.html.html'; const dst='dist/'+f+'.html'; if(fs.existsSync(src)) { fs.renameSync(src,dst); console.log('Renamed',src,'->',dst); } });\"",
    "preview": "vite preview",
    "storybook": "npm run storybook -w @kinzal-net/ui",
    "build-storybook": "npm run build-storybook -w @kinzal-net/ui"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k-kinzal/kinzal.net.git"
  },
  "author": "k-kinzal",
  "homepage": "http://kinzal.net/",
  "private": true,
  "devDependencies": {
    "@kinzal-net/vite-plugin-face-crop": "*",
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/html-vite": "^10.0.8",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "lightningcss": "^1.30.2",
    "lodash": "^4.17.21",
    "postcss": "^8.4.49",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-imagetools": "^9.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-react-ssg": "^0.8.9"
  },
  "dependencies": {
    "@kinzal-net/ui": "*",
    "echo-js": "^1.7.3",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.28.0"
  }
}
