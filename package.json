{
  "name": "kinzal.net",
  "version": "1.0.0",
  "private": true,
  "description": "Illustration portfolio of kinzal.",
  "homepage": "http://kinzal.net/",
  "repository": {
    "type": "git",
    "url": "https://github.com/k-kinzal/kinzal.net.git"
  },
  "author": "k-kinzal",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "vite-react-ssg build",
    "build-storybook": "storybook build",
    "dev": "vite",
    "format": "prettier --write \"app/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,css,json}\"",
    "lint": "eslint app/ && stylelint \"app/**/*.css\"",
    "lint:fix": "eslint app/ --fix && stylelint \"app/**/*.css\" --fix",
    "preview": "vite preview",
    "quality": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "storybook": "storybook dev",
    "test": "vitest run",
    "test:vrt": "playwright test",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@kinzal-net/ui": "workspace:*",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@kinzal-net/vite-plugin-face-crop": "workspace:*",
    "@kinzal-net/vite-plugin-image-list": "workspace:*",
    "@kinzal-net/vite-plugin-image-variants": "workspace:*",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-themes": "^10.1.4",
    "@storybook/react-vite": "^10.1.4",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.1",
    "lightningcss": "^1.30.2",
    "postcss": "^8.4.49",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^10.1.10",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-imagetools": "^9.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-react-ssg": "^0.8.9",
    "vitest": "^3.2.4"
  }
}
